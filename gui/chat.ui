<?xml version='1.0' encoding='utf-8'?>
<interface version="1.2">
  <object class="tk.Toplevel" id="mainwindow">
    <property name="geometry">800x600</property>
    <property name="height">200</property>
    <property name="minsize">400|300</property>
    <property name="resizable">both</property>
    <property name="title" translatable="yes">Chat</property>
    <property name="width">200</property>
    <child>
      <object class="ttk.Frame" id="Frame_1">
        <property name="height">200</property>
        <property name="padding">5</property>
        <property name="width">200</property>
        <layout manager="pack">
          <property name="expand">true</property>
          <property name="fill">both</property>
          <property name="side">top</property>
        </layout>
        <containerlayout manager="grid">
          <property name="anchor">center</property>
          <property type="col" id="0" name="uniform">0</property>
          <property type="col" id="0" name="weight">1</property>
          <property type="row" id="0" name="weight">1</property>
        </containerlayout>
        <child>
          <object class="pygubu.builder.widgets.tkinterscrolledtext" id="chatbox">
            <property name="background">#c0c0c0</property>
            <property name="state">disabled</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="columnspan">2</property>
              <property name="row">0</property>
              <property name="rowspan">1</property>
              <property name="sticky">nsew</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="messagebox">
            <property name="textvariable">string:message_box</property>
            <bind sequence="&lt;Key&gt;" handler="on_enter" add="" />
            <layout manager="grid">
              <property name="column">0</property>
              <property name="padx">3</property>
              <property name="pady">5</property>
              <property name="row">1</property>
              <property name="sticky">nsew</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="sendbutton">
            <property name="command" type="command" cbtype="simple">on_send</property>
            <property name="text" translatable="yes">Send</property>
            <layout manager="grid">
              <property name="column">1</property>
              <property name="padx">3</property>
              <property name="pady">5</property>
              <property name="row">1</property>
              <property name="sticky">nsew</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
  <object class="tk.Menu" id="mainmenu">
    <property name="tearoff">false</property>
    <child>
      <object class="tk.Menuitem.Submenu" id="menufile">
        <property name="label" translatable="yes">File</property>
        <property name="tearoff">false</property>
        <child>
          <object class="tk.Menuitem.Command" id="menunew">
            <property name="command" type="command" cbtype="with_wid">on_new</property>
            <property name="label" translatable="yes">New</property>
          </object>
        </child>
        <child>
          <object class="tk.Menuitem.Separator" id="separator1" />
        </child>
        <child>
          <object class="tk.Menuitem.Command" id="menuquit">
            <property name="command" type="command" cbtype="with_wid">on_quit</property>
            <property name="label" translatable="yes">Quit</property>
            <property name="state">normal</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="tk.Menuitem.Submenu" id="menuedit">
        <property name="label" translatable="yes">Edit</property>
        <property name="tearoff">false</property>
        <child>
          <object class="tk.Menuitem.Submenu" id="menutextsize">
            <property name="label" translatable="yes">Text Size</property>
            <property name="tearoff">false</property>
            <child>
              <object class="tk.Menuitem.Radiobutton" id="radiobutton1">
                <property name="label" translatable="yes">Small</property>
              </object>
            </child>
            <child>
              <object class="tk.Menuitem.Radiobutton" id="radiobutton2">
                <property name="label" translatable="yes">Medium</property>
              </object>
            </child>
            <child>
              <object class="tk.Menuitem.Radiobutton" id="radiobutton3">
                <property name="label" translatable="yes">Large</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="tk.Menuitem.Submenu" id="menuhelp">
        <property name="label" translatable="yes">Help</property>
        <property name="tearoff">false</property>
        <child>
          <object class="tk.Menuitem.Command" id="menuabout">
            <property name="command" type="command" cbtype="with_wid">on_about</property>
            <property name="label" translatable="yes">About</property>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
